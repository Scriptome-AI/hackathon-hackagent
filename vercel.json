{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "app.js" }  
  ]
  "rewrites": [
    {
      "source": "/hackathon-hackagent/:path*",
      "destination": "https://your-hackathon-project.vercel.app/hackathon-hackagent/:path*"
    }
  ]
}
